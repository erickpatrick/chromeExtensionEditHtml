  {
    "manifest_version": 2,
    "name": "Make page editable",
    "description": "edit inline and save new page's DOM state",
    "version": "1.0.0",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [{
        "matches": [
            "https://yourwebsitehere.xyz/*"
        ],
        "css": [
            "style.css"
        ],
        "js": [
            "content.js"
        ]
    }]
}
